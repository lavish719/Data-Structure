#include <iostream>
using namespace std;

int main() {
	int numbers[100];
	int size;

	cout << "Enter number of elements: ";
	cin >> size;


	cout << "Enter the elements:";
	for (int i = 0; i < size; i++) {
		cin >> numbers[i];
	}

	
	for (int i = 0; i < size; i++) { // outer loop i picks each element one by one
		for (int j = i + 1; j < size; ) { //checks all elements after i to see if any are the same
			if (numbers[i] == numbers[j]) {
				
				for (int k = j; k < size - 1; k++) {
					numbers[k] = numbers[k + 1];
				}
				size--; 
				
			} else {
				j++; 
			}
		}
	}


	cout << "Array after removing duplicates:\n";
	for (int i = 0; i < size; i++) {
		cout << numbers[i] << " ";
	}

	return 0;
}
